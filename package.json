{
  "name": "ecs-starter-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start:dev": "cross-env NODE_ENV=development tsx src/server.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "clean:all": "rimraf dist coverage node_modules/.cache",
    "clean:build": "rimraf dist/**/*.map dist/**/*.tsbuildinfo",
    "build:fast": "rimraf dist && swc src -d dist",
    "build:prod": "npm run clean:all && swc src -d dist --env-name production",
    "debug": "tsx --inspect src/server.ts",
    "version:patch": "node scripts/bump-version.js patch",
    "version:minor": "node scripts/bump-version.js minor",
    "version:major": "node scripts/bump-version.js major",
    "release": "npm run test && npm run version:patch && git add . && git commit -m \"chore: bump version\" && git push",
    "health": "node scripts/health-check.js"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.808.0",
    "@aws-sdk/client-s3": "^3.808.0",
    "@fastify/circuit-breaker": "^4.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/otel": "^0.8.0",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/routes": "^6.0.2",
    "@fastify/routes-stats": "^4.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "fastify": "^5.3.3",
    "fastify-metrics": "^12.1.0",
    "moment": "^2.30.1",
    "mysql2": "^3.14.1",
    "winston": "^3.17.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.24",
    "@types/node": "^22.15.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.32.1"
  },
  "private": true
}
